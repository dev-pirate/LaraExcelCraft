{
    "name": "dev-pirate/lara-excel-craft",
    "description": "Laravel package to export / import excel sheets",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DevPirate\\LaraExcelCraft\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Meherbs",
            "email": "meherbensalah4@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-cors": "*",
        "phpoffice/phpspreadsheet": "^1.29",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "mockery/mockery": "^1.2"
    },
    "extra": {
        "laravel": {
            "aliases": {
                "LaraExcelCraft": "DevPirate\\LaraExcelCraft\\Facades\\LaraExcelCraft"
            },
            "providers": [
                "DevPirate\\LaraExcelCraft\\Providers\\LaraExcelCraftProvider"
            ]
        }
    },
}
